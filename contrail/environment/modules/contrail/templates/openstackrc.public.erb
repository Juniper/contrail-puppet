export OS_USERNAME=<%= @keystone_admin_user %>
export OS_PASSWORD=<%= @keystone_admin_password %>
export OS_TENANT_NAME=<%= @keystone_admin_tenant %>
export OS_AUTH_URL=<%= @keystone_auth_protocol %>://<%= @keystone_mgmt_ip%>:5000/v2.0/
export OS_REGION_NAME=<%= @keystone_region_name %>
export OS_NO_CACHE=1
export OS_VOLUME_API_VERSION=2
export OS_CACERT=/etc/keystone/ssl/certs/ca.pem
export OS_INTERFACE=public
export OS_ENDPOINT_TYPE=public
